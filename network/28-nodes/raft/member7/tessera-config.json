  },
  {
    "app":"Q2T",
    "enabled": true,
    "serverAddress": "http://<HOSTNAME>:9101",
    "sslConfig": {
      "tls": "OFF"
    },
    "communicationType" : "REST"
  },
  {
    "app":"P2P",
    "enabled": true,
    "serverAddress": "http://<HOSTNAME>:9000",
    "sslConfig": {
      "tls": "OFF"
    },
    "communicationType" : "REST"
  }
  ],
  "peer": [
      {
          "url": "http://<HOSTNAME>:9000"
      }
  ],
  "keys": {
    
    "keyData": [
      {
        "privateKeyPath": "./tessera.key",
        "publicKeyPath": "./tessera.pub"
      }
    ]
  },
  "alwaysSendTo": []
}
